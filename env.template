# Dialogflow Configuration Template
# Copy this file to .env and fill in your actual values

# Your Google Cloud Project ID (found in Google Cloud Console)
DIALOGFLOW_PROJECT_ID=your-project-id-here

# Session ID (can be any unique string per user session)
DIALOGFLOW_SESSION_ID=default-session

# Language code (e.g., 'en' for English, 'es' for Spanish)
DIALOGFLOW_LANGUAGE_CODE=en

# Dialogflow Access Token
# (leave blank when using a service-account key)
DIALOGFLOW_ACCESS_TOKEN=

# Dialogflow Knowledge Base ID
DIALOGFLOW_KNOWLEDGE_BASE_ID=MTU3Nzc5ODcxODU2NjExODE5NTM

# Absolute path to your service-account JSON key
# Example: C:/keys/querybee-key.json
GOOGLE_APPLICATION_CREDENTIALS=C:/keys/querybee-key.json